{
  "name": "@rqjs/rqcomponents",
  "version": "0.2.2",
  "description": "A Component Library for Ricequant.",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/anson09/rqcomponents.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "./scripts/updep.sh && parcel watch ./demo/index.html --no-autoinstall --out-dir demo/dist --cache-dir demo/.cache --public-url /rqcomponents/",
    "dev": "./scripts/updep.sh && rollup -w -c rollup.config.js",
    "build": "./scripts/updep.sh && rm -rf lib && rollup -c rollup.config.js",
    "postbuild": "./scripts/postbuild.sh",
    "lint": "./scripts/lint.sh",
    "clean": "rm -rf demo/.cache demo/dist",
    "release": "standard-version --no-verify",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.8.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.3",
    "postcss": "^8.2.1",
    "postcss-css-variables": "^0.17.0",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.35.1",
    "rollup-plugin-image-files": "^1.4.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.30.0",
    "standard-version": "^9.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "@rqjs/rqthemes": "^2.0.0-next.1",
    "axios": "^0.21.0",
    "lodash": "^4.17.20"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  }
